package br.com.b2ti.exception;

/**
 *
 * @author max
 */
public class PermissaoException extends Exception{

    public PermissaoException() {
    }

    public PermissaoException(String message) {
        super(message);
    }

    public PermissaoException(String message,Exception ex) {
        super(message,ex);
    }

    @Override
    public String getMessage() {
        return "Problema encontrado:"+super.getMessage();
    }

    @Override
    public String toString() {
        return getMessage();
    }


}
